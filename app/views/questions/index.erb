
<div class="center-text">

<div class="questions-title"><h1>Stackoverflow</h1></div>

<% @questions.each do |question| %>
  <h3><a href="/questions/<%=question.id%>"><%=question.title%></a></h3>
<p>Votes: <%=sum_votes(question.votes)%></p>
  <strong>Tags</strong><%question.tags.each do |tag|%>
  <a class="tagg" href="/tags/<%=tag.id%>"><%=tag.name%>&nbsp</a>
  <%end%>

  <hr>
<% end %>
</div>
