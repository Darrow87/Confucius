
<div><a href="/questions/new">New Question</a></div>
<div><a href="/users">Show All Users</a></div>
<h1>Questions</h1>
<% @questions.each do |question| %>
  <h3><a href="/questions/<%=question.id%>"><%=question.title%></a></h3>
  <strong>Tags</strong><%question.tags.each do |tag|%>
  <div><a href="/tags/<%=tag.id%>"><%=tag.name%></a></div>
  <%end%>

<% end %>
